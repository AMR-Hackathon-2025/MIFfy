params {
    // TODO nf-core: Specify your pipeline's command line flags
    // Input options
    input                      = null

    // Boilerplate options
    database  = "/shared/public/db/bakta/latest/db"
    outdir                     = "output"
    output		                 = false
    out_dir		                 = false
    trace_report_suffix        = new java.util.Date().format('yyyy-MM-dd_HH-mm-ss')
    tracedir                   = "${params.outdir}/pipeline_info/${trace_report_suffix}"
    publish_dir_mode           = 'copy'
    email                      = null
    email_on_fail              = null
    plaintext_email            = false
    monochrome_logs            = false
    hook_url                   = null
    help                       = false
    version                    = false
    validate_params            = true
    show_hidden_params         = false
    schema_ignore_params       = null

    // Max resource options
    // Defaults only, expecting to be overwritten
    max_memory                 = '128.GB'
    max_cpus                   = 16
    max_time                   = '240.h'

    help = false
    version = false

    store_dir = "store_dir"

    climb = false
    local = false

    unique_id = null
    fastq = null

    skip_dehosting = false

    disable_ping = false
    threads = 2
    process_label = "miffy"
    monochrome_logs = false

    validate_params = true
    show_hidden_params = false

    wf {
      example_cmd = [
          "--fastq test_data/barcode01/reads.fastq.gz",
      ]
      agent = null
      container = "biowilko/scylla"
      container_version = "1.2.1"
    }
}